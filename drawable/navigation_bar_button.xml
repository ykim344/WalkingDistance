<?xml version="1.0" encoding="utf-8"?>
<selector
 xmlns:android="http://schemas.android.com/apk/res/android">
 <item android:state_pressed="true" >
 <shape>
 <gradient
 android:endColor="#ff999999"
 android:startColor="#ff333333"
 android:angle="270" />
 <stroke
 android:width="1px"
 android:color="#FF000000" />
 <corners
 android:radius="6dp" />
 <padding
 android:left="10dp"
 android:top="10dp"
 android:right="10dp"
 android:bottom="10dp" />
 </shape>
 </item>
 <item>
 <shape>
 <gradient
 android:startColor="#ff7684c1"
 android:endColor="#ff495a95"
 android:angle="270" />
 <stroke
 android:width="1px"
 android:color="#FF000000" />
 <corners
 android:radius="6dp" />
 <padding
 android:left="10dp"
 android:top="10dp"
 android:right="10dp"
 android:bottom="10dp" />
 </shape>
 </item>
</selector>
